{
  "name": "imicros-exchange",
  "version": "0.0.1",
  "author": "Andreas Leinen <andreas.leinen@imicros.de> (www.imicros.de)",
  "license": "MIT",
  "description": "Collection of Moleculer services for file exchange and message handling",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --no-cache",
    "test-local": "jest --coverage --no-cache --config=local.js",
    "test-files": "jest test/files.spec.js --no-cache --config=local.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/*.js",
      "lib/util/*.js",
      "!lib/globals.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dev/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dev/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/al66/imicros-exchange.git"
  },
  "keywords": [
    "imicros",
    "moleculer",
    "file exchange"
  ],
  "devDependencies": {
    "coveralls": "^3.0.4",
    "imicros-acl": "0.0.3",
    "imicros-rules-compiler": "0.0.8",
    "jest": "^24.8.0",
    "moleculer": "^0.13.9",
    "nats": "^1.3.0"
  },
  "dependencies": {
    "JSONStream": "^1.3.5",
    "event-stream": "^4.0.1",
    "json-templater": "^1.2.0",
    "json-templates": "^2.4.0",
    "json4json": "^1.1.0",
    "lodash": "^4.17.14",
    "neo4j-driver": "^1.7.5",
    "split": "^1.0.1",
    "stream-chain": "^2.1.0",
    "uuid": "^3.3.2"
  }
}
